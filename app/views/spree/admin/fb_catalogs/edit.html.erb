<% content_for :page_actions do %>
	<li>
		<%= link_to Spree.t(:back_to_fb_catalogs_list), spree.admin_fb_catalogs_path, class: 'button' %>
	</li>
<% end %>

<%= render :partial => 'spree/shared/error_messages', :locals => { :target => @fb_catalog } %>
<%= render :partial => 'spree/admin/shared/fb_catalog_tabs', :locals => { :current => 'Facebook Catalog' } %>

<% admin_breadcrumb(Spree.t(:details)) %>

<%= form_for [:admin , @fb_catalog], :html => { :multipart => true } do |f| %>
  <fieldset class="no-border-top">
    <%= render 'form', f: f %>
    <div class="clear"></div>
    <div class="form-buttons filter-actions actions" data-hook="buttons">
	  <%= button Spree.t('actions.update') %>
	  <%= link_to Spree.t('actions.cancel'), collection_url, :class => 'button' %>
	</div>
  </fieldset>
<% end %>
